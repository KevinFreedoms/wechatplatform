<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sr.platform.server.orderManagement.dao.IWxLocDao">
<select id="getLoc" resultType="com.sr.platform.server.orderManagement.bean.WxLoc">
    SELECT
      sid,
      branch_name
    FROM
      wx_loc
    ORDER BY
      sid
</select>

</mapper>